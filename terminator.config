[global_config]
  window_state = maximise
  handle_size = 1
  title_hide_sizetext = True
  dbus = True
  enabled_plugins = ActivityWatch, LaunchpadCodeURLHandler
  geometry_hinting = False
  inactive_color_offset = 0.76
[keybindings]
  full_screen = <Ctrl><Shift>F11
[profiles]
  [[default]]
    scrollbar_position = hidden
    use_system_font = False
    font = Monospace Bold 10
    background_darkness = 1.39
    background_image = None
    scroll_on_output = False
    show_titlebar = False
    copy_on_selection = True
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
